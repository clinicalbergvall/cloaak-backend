{
  "name": "cleancloak",
  "private": true,
  "version": "0.0.1",
  "main": "firebase-functions.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "preview": "vite preview",
    "android:sync": "npm run build:prod && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build:prod && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:build:debug": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:install": "npm run android:build:debug && cd android && ./gradlew installDebug",
    "android:run": "npm run android:build:debug && npx cap run android",
    "ios:sync": "npm run build:prod && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:android": "cd android && ./gradlew clean",
    "optimize": "npm run clean && npm install && npm run build:prod"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.1",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/geolocation": "^7.1.7",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/push-notifications": "^7.0.4",
    "@capacitor/splash-screen": "^7.0.4",
    "@types/leaflet": "^1.9.21",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "firebase": "^12.7.0",
    "helmet": "^8.1.0",
    "intasend-node": "^1.1.2",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "mongoose": "^9.0.2",
    "morgan": "^1.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.9.6",
    "serverless-http": "^4.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^7.2.2"
  }
}
