{
  "name": "cleancloak",
  "private": true,
  "version": "0.0.1",
  "main": "firebase-functions.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "type-check": "tsc -b --noEmit",
    "build-no-check": "vite build",
    "full-build": "npm run type-check && vite build",
    "skip-type-build": "vite build",
    "preview": "vite preview",
    "android:sync": "npm run build:prod && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run build:prod && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:build:debug": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:install": "npm run android:build:debug && cd android && ./gradlew installDebug",
    "android:run": "npm run android:build:debug && npx cap run android",
    "ios:sync": "npm run build:prod && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:android": "cd android && ./gradlew clean",
    "optimize": "npm run clean && npm install && npm run build:prod",
    "postinstall": "echo 'Postinstall: Admin setup will run on server start'"
  },
  "dependencies": {
    "@capacitor/android": "^5.0.0",
    "@capacitor/cli": "^5.0.0",
    "@capacitor/ios": "^8.0.0",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@types/leaflet": "^1.9.21",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^12.0.0",
    "helmet": "^8.1.0",
    "intasend-node": "^1.1.2",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "mongoose": "^9.0.2",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.30.3",
    "serverless-http": "^4.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@capacitor/app": "^8.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/core": "^5.0.0",
    "@capacitor/geolocation": "^5.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@types/google.maps": "^3.58.1",
    "@types/history": "^4.7.11",
    "@types/node": "^20.19.28",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "http-proxy-middleware": "^3.0.5",
    "mongodb-memory-server": "^11.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  }
}
